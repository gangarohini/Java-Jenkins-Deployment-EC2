pipeline{
    agent any
    tools{
        maven "Maven 3.9"
    }
    stages{
        stage('Build Maven'){
            steps{
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'GithubCreds', url: 'https://github.com/gangarohini/Java-Jenkins-Deployment-EC2.git']])
                sh "mvn -Dmaven.test.failure.ignore=true clean package"
            }
        }
        }
    }